/*
 * helloworld.c: simple test application
 */

#include <stdio.h>
#include "platform.h"

int fib(int n)
{
	if(n < 2)
		return 1;
	else
		return fib(n - 1) + fib(n - 2);
}

int main()
{
    init_platform();

	int n = 0;
	for(n = 0; n < 5; n++)
	{
	    xil_printf("Hello World\n\r");
    	xil_printf("%d \n\r", fib(n));
	}

    cleanup_platform();

    return 0;
}
