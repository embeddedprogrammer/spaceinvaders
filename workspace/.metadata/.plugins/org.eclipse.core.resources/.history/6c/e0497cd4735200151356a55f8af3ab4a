/*
 * helloworld.c: simple test application
 */

#include <stdio.h>
#include "platform.h"

int fib(int n)
{
	if(n < 2)
		return 1;
	else
		return fib(n - 1) + fib(n - 2);
}

int main()
{
    init_platform();

    xil_printf("Hello World\n\r");
    xil_printf("%d", fib(0));
    xil_printf("%d", fib(1));
    xil_printf("%d", fib(2));
    xil_printf("%d", fib(3));
    xil_printf("%d", fib(4));

    cleanup_platform();

    return 0;
}
